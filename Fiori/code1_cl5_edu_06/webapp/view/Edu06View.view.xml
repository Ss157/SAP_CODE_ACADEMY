<mvc:View controllerName="code1.cl5.edu06.code1cl5edu06.controller.Edu06View"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <VBox>
            <Button text="{/gs_button/text}" icon="{/gs_button/icon}" enabled="{/gs_button/enabled}"/>
            <Input placeholder="{Input>/gs_input/place}" width="{Input>/gs_input/width}" description="{Input>/gs_input/desc}" />
            <Label text="{Label>/gs_label/text}" width="{Label>/gs_label/width}" design="{Label>/gs_label/design}" />
            <Text text="{/gs_text/text}" width="{/gs_text/width}" textAlign="{/gs_text/align}" />
        </VBox>
    </Page>
</mvc:View>